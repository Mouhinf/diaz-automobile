rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /cars/{carId} {
      allow read, write: if true; // Permet la lecture et l'écriture pour la collection 'cars'
    }
    // Règle par défaut restrictive pour les autres collections (si vous en avez)
    match /{document=**} {
      allow read, write: if false;
    }
  }
}